      <ReturnValue>
        <ReturnType>Microsoft.Office.Interop.Excel.Scenario</ReturnType>
      </ReturnValue>
      <Parameters>
        <Parameter Name="Name" Type="System.String" />
        <Parameter Name="ChangingCells" Type="System.Object" />
        <Parameter Name="Values" Type="System.Object" />
        <Parameter Name="Comment" Type="System.Object" />
        <Parameter Name="Locked" Type="System.Object" />
        <Parameter Name="Hidden" Type="System.Object" />
      </Parameters>
      <Docs>
        <param name="Name">必需的 <b>字符串</b> 。 方案名。</param>
        <param name="ChangingCells">必需的 <b>对象</b>。 <see cref="T:Microsoft.Office.Interop.Excel.Range" />引用方案的更改单元格的对象。</param>
        <param name="Values">可选 <b>对象</b>。 包含 中单元格的方案值的数组 <paramref name="ChangingCells" /> 。 如果省略此参数，则假定方案值为 中单元格的当前值 <paramref name="ChangingCells" /> 。</param>
        <param name="Comment">可选 <b>对象</b>。 一个指定方案批注文本的字符串。 添加新方案时，作者的姓名和日期自动添加在批注文本的开始部分。</param>
        <param name="Locked">可选 <b>对象</b>。 如果为 <b>True</b>，则锁定方案以防更改。 默认值为 <b>True</b>。</param>
        <param name="Hidden">可选 <b>对象</b>。 如果为 <b>True</b>，则隐藏方案。 默认值为 <b>False</b>。</param>
        <summary>新建方案并将其添加到当前工作表可用的方案列表中。</summary>
        <returns>To be added.</returns>
        <remarks><para>方案名称必须是唯一的；如果试图用已经在使用的名称创建方案，Microsoft Excel 会产生错误。</para>
          <para> </para></remarks>
      </Docs>
    </Member>
